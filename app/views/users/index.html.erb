<div class="index-container">
  <div id="index-wrapper" class="wrapper clearfix">
    <%= render "partials/flash_messages" %>
    <h2>Users</h2>
    <ol>
      <% @users.each do |user| %>
        <%= link_to render(partial: 'users/user', locals: {user: user}), user %>
      <% end %>
    </ol>
    <%= will_paginate %>
  </div>

  <br>

  <p class="create-new-link"><%= link_to 'Back To Home', root_path %></p>
</div>
