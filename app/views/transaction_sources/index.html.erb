<p id="notice"><%= notice %></p>

<h1>Transaction Sources</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transaction_sources.each do |transaction_source| %>
      <tr>
        <td><%= link_to 'Show', transaction_source %></td>
        <td><%= link_to 'Edit', edit_transaction_source_path(transaction_source) %></td>
        <td><%= link_to 'Destroy', transaction_source, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transaction Source', new_transaction_source_path %>
